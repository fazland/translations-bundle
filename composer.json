{
    "name": "fazland/translations-bundle",
    "description": "Crazy simple translations bundle for Symfony",
    "require": {
        "php": ">=5.5",
        "symfony/intl": "^2.7|^3.0",
        "symfony/framework-bundle": "^2.7|^3.0",
        "symfony/twig-bundle": "^2.7|^3.0"
    },
    "require-dev": {
        "sensio/framework-extra-bundle": "^3.0",
        "symfony/browser-kit": "^2.7|^3.0"
    },
    "license": "MIT",
    "type": "symfony-bundle",
    "authors": [
        { "name": "Giovanni Albero", "email": "giovannialbero.solinf@gmail.com" },
        { "name": "Alessandro Chitolina", "email": "alekitto@gmail.com" }
    ],
    "autoload": {
        "psr-4": {
            "Fazland\\TranslationsBundle\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fazland\\TranslationsBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
